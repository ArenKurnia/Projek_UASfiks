@{ Layout = "_LayoutUser"; }

@model IEnumerable<Tahun>

@{ ViewData["Title"] = "Index"; }

<br />
<br />

<div class="card mb-4">
    <div class="card-header">
        <i class="fas fa-table me-1"></i>
        Data Tahun
    </div>
    <div class="card-body">
        <table class="table" id="datatablesSimple">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Id_angkatan)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Tahun_angkatan)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Nama_angkatan)
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id_angkatan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tahun_angkatan)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nama_angkatan)
        </td>
        <td>
            <a class="btn btn-dark" style="color:white" asp-action="Detail" asp-route-id="@item.Id_angkatan">Detail</a>
        </td>
    </tr>}
            </tbody>
        </table>
    </div>
</div>

