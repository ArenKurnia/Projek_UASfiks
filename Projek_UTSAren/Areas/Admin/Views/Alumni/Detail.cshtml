@model Alumni

@{ Layout = "_LayoutAdmin"; }

@{ ViewData["Title"] = "Detail"; }

<br />
<h2>Detail Alumni</h2>
<hr />
<div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.NIM)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.NIM)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Nama_alumni)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Nama_alumni)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Tahun_angkatan)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Tahun_angkatan)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Jenis_kelamin)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Jenis_kelamin)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Tempat_lahir)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Tempat_lahir)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Tanggal_lahir)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Tanggal_lahir)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Pekerjaan)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Pekerjaan)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Alamat)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Alamat)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Telp)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Telp)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Foto)
        </dt>
        <dd class="col-sm-10">
            <img src="@Model.Foto" width="100" alt="Alternate Text" />
        </dd>
    </dl>
</div>
<br />
<br />
<div>
    <a class="btn btn-dark" style="color:white" asp-action="Update" asp-route-id="@Model.NIM">Edit</a> 
    <a class="btn btn-dark" style="color:white" asp-action="Index">Kembali</a>
</div>
