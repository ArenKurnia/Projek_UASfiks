@model Tahun

@{ Layout = "_LayoutAkun"; }

<h2 style="text-align:center">Edit</h2>

<h5 style="text-align:center">Data Tahun</h5>
<hr />
<div class="row">
    <div class="card-body">
        <form asp-action="Update">
            <div class="form-group">
                <label asp-for="Id_angkatan" class="control-label"></label>
                <input asp-for="Id_angkatan" class="form-control" />
                <span asp-validation-for="Id_angkatan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Tahun_angkatan" class="control-label"></label>
                <input asp-for="Tahun_angkatan" class="form-control" />
                <span asp-validation-for="Tahun_angkatan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Nama_angkatan" class="control-label"></label>
                <textarea asp-for="Nama_angkatan" class="form-control"></textarea>
                <span asp-validation-for="Nama_angkatan" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a class="btn btn-dark" style="color:white" asp-action="Index">Kembali</a>
</div>
<br />
<br />
<br />

